# Give your application a unique name (no spaces)
APP_NAME=${APP_NAME}
KEY=${KEY}

# sets debug information on the loggers
DEBUG=true

# allowed url's while in maintenance mode (must be comma seperated: admin,login)
ALLOWED_URLS=

#RPC port
RPC_PORT=

# the server name, e.g, www.{$APP_NAME}.com
SERVER_NAME=${APP_NAME}
SERVER_SECURE=false
SERVER_HOST=
SERVER_PORT=

# database config supported: [postgres, mysql, sqlite]
DATABASE_TYPE=
DATABASE_NAME=${APP_NAME}
DATABASE_USER=
DATABASE_PASS=
DATABASE_HOST=
DATABASE_PORT=
# postgres values: [enable/disable], mysql values: [true/false]
DATABASE_SSL_MODE=

# cache type: [redis, badger]
CACHE_TYPE=
CACHE_PREFIX=${APP_NAME}
CACHE_HOST=
CACHE_PORT=
CACHE_PASS=

# cooking seetings: {name: string}, {minutes: int}, {persist: bool}, {secure: bool}
COOKIE_NAME=${APP_NAME}
COOKIE_DOMAIN=
COOKIE_LIFETIME=
COOKIE_PERSIST=
COOKIE_SECURE=

# session store: [cookie (default), redis, badger, postgres, sqlite, mysql]
SESSION_TYPE=

# mail settings smtp -> later API
MAILER=

# mail settings for smtp
SMTP_DOMAIN=
SMTP_HOST=
SMTP_PORT=
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_FROM=
SMTP_FROM_NAME=
SMTP_ENCRYPTION=
SMTP_AUTH_METHOD=

# template engine: [go, jet]
RENDERER=